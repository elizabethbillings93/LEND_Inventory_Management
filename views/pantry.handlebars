          
<link rel="stylesheet" href="/css/products.css">     
<div class="product-items">                
      {{#each products}}
            <div id="list">
                  <img class="product-image" src="{{image_link}}" >
                  <label for="product-count" onkeypress="return enterKeyPressed(event)" class="product-description">{{name}}</label>
                  <input class="product-count"  placeholder="{{quantity}}">
            </div>
      {{/each}}
</div>         

                   